{
  "$schema": "https://allpath-runner.dev/schema/v1.json",
  "name": "senjutsu-coding-agent",
  "version": "1.0.0",
  "language": "python",
  "entry_point": "main.py",
  "description": {
    "fr": "Agent de codage IA avec précision absolue — Byakugan × Mode Sage × Jōgan × RAG",
    "en": "AI coding agent with absolute precision — Byakugan × Mode Sage × Jōgan × RAG Booster"
  },
  "functions": [
    {
      "name": "run",
      "description": {
        "fr": "Pipeline complet : analyse triple perception + sélection skills + livrable",
        "en": "Full pipeline: triple perception analysis + skill selection + deliverable"
      },
      "params": [
        {"name": "task", "type": "string", "description": {"en": "Development task to accomplish"}},
        {"name": "api_key", "type": "string", "description": {"en": "Groq/OpenAI API key (or set env var)"}},
        {"name": "model", "type": "string", "description": {"en": "LLM model to use"}},
        {"name": "provider", "type": "string", "description": {"en": "LLM provider: groq or openai"}}
      ],
      "returns": {"type": "object", "description": {"en": "JSON with byakugan, mode_sage, jougan, execution, timing"}}
    },
    {
      "name": "byakugan",
      "description": {"en": "Run only Byakugan structural vision"},
      "params": [
        {"name": "task", "type": "string"},
        {"name": "api_key", "type": "string"}
      ],
      "returns": {"type": "object"}
    },
    {
      "name": "check_skill",
      "description": {"en": "Security check a skill's content for malicious patterns"},
      "params": [
        {"name": "skill_content", "type": "string"}
      ],
      "returns": {"type": "object", "description": {"en": "{safe: bool, violations: [str]}"}}
    },
    {
      "name": "version",
      "description": {"en": "Return package version"},
      "returns": {"type": "object"}
    }
  ],
  "dependencies": {
    "runtime": ["python3>=3.8"],
    "packages": ["senjutsu>=1.0.0b1"]
  },
  "license": "MIT",
  "author": {
    "name": "Daouda Abdoul Anzize",
    "email": "anzize.contact@proton.me",
    "url": "https://tryboy869.github.io/daa"
  },
  "repository": "https://github.com/Tryboy869/senjutsu-coding-agent",
  "homepage": "https://github.com/Tryboy869/senjutsu-coding-agent",
  "tags": ["ai", "coding-agent", "rag", "llm", "precision-absolute", "byakugan", "senjutsu"]
}
