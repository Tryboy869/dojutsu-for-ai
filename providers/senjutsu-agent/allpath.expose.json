{
  "$schema": "https://allpath-runner.dev/schema/v1.json",
  "name": "senjutsu-agent",
  "version": "1.1.0b1",
  "language": "python",
  "entry_point": "main.py",
  "description": {
    "fr": "Agent IA Précision Absolue — Byakugan × Mode Sage × Jōgan × RAG Booster. Génère du code production-ready depuis une tâche vague via un pipeline 5 étapes.",
    "en": "Precision Absolute AI coding agent — Byakugan × Mode Sage × Jōgan × RAG Booster. Generates production-ready code from a vague task via a 5-step pipeline."
  },
  "functions": [
    {
      "name": "run",
      "description": {"en": "Full 5-step pipeline: structural vision + systemic coherence + trajectory anticipation + RAG skills + code generation"},
      "params": [
        {"name": "task",     "type": "string",  "description": {"en": "Development task in natural language"}},
        {"name": "api_key",  "type": "string",  "description": {"en": "LLM API key (or GROQ_API_KEY env var)"}},
        {"name": "provider", "type": "string",  "description": {"en": "groq (default) or openai"}},
        {"name": "model",    "type": "string",  "description": {"en": "Model name (optional, uses provider default)"}},
        {"name": "verbose",  "type": "string",  "description": {"en": "true/false — print pipeline steps"}}
      ],
      "returns": {"type": "object", "description": {"en": "{byakugan, mode_sage, jougan, execution, skills_used, timing, total_time}"}}
    },
    {
      "name": "byakugan",
      "description": {"en": "Structural vision step only (1 LLM call, faster)"},
      "params": [
        {"name": "task",     "type": "string"},
        {"name": "api_key",  "type": "string"},
        {"name": "provider", "type": "string"},
        {"name": "model",    "type": "string"}
      ],
      "returns": {"type": "object", "description": {"en": "{byakugan, time}"}}
    },
    {
      "name": "skills_list",
      "description": {"en": "List all indexed RAG skills"},
      "params": [],
      "returns": {"type": "string"}
    },
    {
      "name": "skills_count",
      "description": {"en": "Number of indexed RAG skills"},
      "params": [],
      "returns": {"type": "object", "description": {"en": "{count}"}}
    },
    {
      "name": "check_skill",
      "description": {"en": "Security check a skill content"},
      "params": [{"name": "skill_content", "type": "string"}],
      "returns": {"type": "object", "description": {"en": "{safe: bool, violations: []}"}}
    },
    {
      "name": "version",
      "description": {"en": "Package version"},
      "params": [],
      "returns": {"type": "object", "description": {"en": "{version}"}}
    }
  ],
  "dependencies": {
    "runtime": ["python3>=3.8"],
    "packages": ["senjutsu>=1.1.0b1", "groq>=0.9.0"]
  },
  "license": "MIT",
  "author": {
    "name": "Daouda Abdoul Anzize",
    "email": "anzize.contact@proton.me",
    "url": "https://github.com/Tryboy869"
  },
  "repository": "https://github.com/Tryboy869/senjutsu-coding-agent",
  "tags": ["ai-agent", "coding-agent", "llm", "rag", "byakugan", "senjutsu", "allpath", "precision-absolute"]
}
